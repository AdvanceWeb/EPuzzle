<?xml version="1.0"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="web" targetNamespace="web" elementFormDefault="qualified">
<xs:include schemaLocation="model.xsd"/>
	<xs:element name="order">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Date" type="xs:date"/>
				<xs:element name="Size" type="sizes"/>
				<xs:element name="Product" type="products"/>
				<xs:element name="Customer" type="customers"/>
				<xs:element name="Price" type="xs:integer"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="products">
		<xs:sequence>
			<xs:element name="Color" type="materials"/>
			<xs:element name="Shape" type="shapes"/>
			<xs:element  name="Size" type="sizes"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="sizes">
		<xs:sequence>
			<xs:element name="width" type="xs:integer"/>
			<xs:element name="height" type="xs:integer"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="customers">
		<xs:sequence>
			<xs:element name="name" type="xs:string"/>
			<xs:element name="address" type="xs:string"/>
			<xs:element name="phonenumber" type="xs:string"/>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
